<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="com.haijiao.Domain.file.UserFileGroup" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id name="id" type="integer" column="userfilegroupid">
      <generator class="native"></generator>
    </id>
    <property name="groupName" type="string"></property>
    <list name="files">
        <key column="consistof"></key>
        <index column="findex"></index>
        <one-to-many class="com.haijiao.Domain.file.DataFile"></one-to-many>
    </list>
  </class>
</hibernate-mapping>
