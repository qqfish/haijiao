<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="com.haijiao.Domain.bean.Schedule" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id name="id" type="integer" column="scheduleid">
      <generator class="native"></generator>
    </id>
    <many-to-one name="teacher" column="teach"></many-to-one>
    <list name="slices">
        <key column="consist"></key>
        <index column="scheindex"></index>
        <one-to-many class="com.haijiao.Domain.bean.Timeslice"></one-to-many>
    </list>
  </class>
</hibernate-mapping>
