<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="com.haijiao.Domain.bean.Comment" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id name="id" type="integer" column="commentid">
        <generator class="native"></generator>
    </id>
    <one-to-one name="commenter" class="com.haijiao.Domain.bean.User"></one-to-one>
    <one-to-one name="commentee" class="com.haijiao.Domain.bean.User"></one-to-one>
    <property name="content" type="string"></property>
    <property name="score" type="integer"></property>
  </class>
</hibernate-mapping>
