<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="com.haijiao.Domain.bean.Timeslice" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id name="id" type="integer" column="timesliceid">
      <generator class="native"></generator>
    </id>
    <property name="day" type="integer"></property>
    <property name="begintime" type="time"></property>
    <property name="endtime" type="time"></property>
    <property name="type" type="string"></property>
    <property name="process" type="string"></property>
    <property name="studentName" type="string"></property>
    <property name="accept" type="boolean"></property>
    <property name="subject" type="string"></property>
  </class>
</hibernate-mapping>
